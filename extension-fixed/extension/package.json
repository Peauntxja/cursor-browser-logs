{
  "name": "cursor-browser-logs",
  "displayName": "Cursor Browser Logs Analyzer",
  "description": "在Cursor中集成浏览器日志分析工具",
  "version": "0.0.1",
  "publisher": "YourPublisherID",
  "author": {
    "name": "Your Name"
  },
  "license": "MIT",
  "pricing": "Free",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/cursor-browser-logs"
  },
  "keywords": [
    "cursor",
    "browser",
    "logs",
    "analyzer",
    "debug"
  ],
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cursor-browser-logs.showLogs",
    "onCommand:cursor-browser-logs.analyzeLogs"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursor-browser-logs.showLogs",
        "title": "Cursor Browser Logs: 查看浏览器日志"
      },
      {
        "command": "cursor-browser-logs.analyzeLogs",
        "title": "Cursor Browser Logs: 分析浏览器日志"
      },
      {
        "command": "cursor-browser-logs.clearLogs",
        "title": "Cursor Browser Logs: 清除日志"
      },
      {
        "command": "cursor-browser-logs.generateFix",
        "title": "Cursor Browser Logs: 生成修复建议"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "browser-logs-explorer",
          "title": "Browser Logs",
          "icon": "resources/browser-logs.svg"
        }
      ]
    },
    "views": {
      "browser-logs-explorer": [
        {
          "id": "browserLogs",
          "name": "浏览器日志"
        },
        {
          "id": "logAnalysis",
          "name": "日志分析"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "cursor-browser-logs.showLogs",
          "when": "view == browserLogs",
          "group": "navigation"
        },
        {
          "command": "cursor-browser-logs.analyzeLogs",
          "when": "view == logAnalysis",
          "group": "navigation"
        },
        {
          "command": "cursor-browser-logs.clearLogs",
          "when": "view == browserLogs",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "cursor-browser-logs.generateFix",
          "group": "browser-logs"
        }
      ]
    },
    "configuration": {
      "title": "Cursor Browser Logs",
      "properties": {
        "cursorBrowserLogs.apiServer": {
          "type": "string",
          "default": "http://localhost:3001",
          "description": "浏览器日志API服务器地址"
        },
        "cursorBrowserLogs.refreshInterval": {
          "type": "number",
          "default": 10000,
          "description": "自动刷新日志的间隔时间（毫秒）"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.35",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "glob": "^7.1.6",
    "mocha": "^8.3.2",
    "typescript": "^4.2.3",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "node-fetch": "^2.6.1"
  }
}
